{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanillaJS and NodeJS",
  "main": "index.js",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '-scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development && webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production && webpack",
    "build:server": "babel src --out-dir build --ignore src/static --ignore src/assets --ignore src/webpack.config.js",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true node build/init.js",
    "prebuild": "rm -rf build",
    "prestart": "npm run build"
  },
  "author": "Jihyun",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^9.8.6",
    "aws-sdk": "^2.836.0",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss-loader": "^4.2.0",
    "pug": "^3.0.0",
    "pug-server": "^0.1.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0"
  }
}
